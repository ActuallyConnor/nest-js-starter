{
  "name": "sdk-nest-js-starter",
  "version": "0.0.1",
  "description": "SDK for the Nest.js API",
  "main": "app.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest --coverage && make-coverage-badge --output-path ./coverage-badge.svg",
    "static-analysis": "eslint --ext .ts src",
    "fix-src": "eslint --ext .ts src --fix",
    "fix-tests": "eslint --ext .ts tests --fix",
    "docs": "npx @compodoc/compodoc -p tsconfig.doc.json -d docs"
  },

  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  },
  "license": "proprietary",
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@compodoc/compodoc": "^1.1.19",
    "@types/fhir": "^0.0.34",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "fhir": "^4.11.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "make-coverage-badge": "^1.2.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0"
  }
}
